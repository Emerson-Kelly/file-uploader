{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true
  },
  "include": ["prisma", "scripts", "types", "node_modules"],
  /*"exclude": ["node_modules"]*/
}
